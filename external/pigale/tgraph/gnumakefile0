####################### MAY NEED EDITING ########################
#Compiler options
CC=g++
#CPU="-mcpu=i686"
#CPU=-m686 
CPU=
OPT=opt
#PIC if wanted shared dynamic libraries
PIC=-fpic

#Unix libraries to be linked (system dependent)
#UNIXLIB =  -L$(MQTDIR)/lib -lqt-mt -L/usr/X11R6/lib -lglut -lGL -lGLU  
UNIXLIB = -L/usr/X11R6/lib -lX11
AWK=awk
####################### END EDITING #########################
PIGALEPATH=..

ifeq ($(OPT),gri)
LINK=-g
else
LINK=
endif

RAN = ranlib
#SAMPLING= -pg
SAMPLING=

L = tgraph
#GENERAL OPTIONS
.SUFFIXES:.cpp .c .opt .dbg .o .gri
IFLAGS = -I$(PIGALEPATH)/incl 

#OPTIMIZED OPTIONS
CFLAGSopt = -O3   $(PIC) $(CPU)
CFLAGSopt += -Wall -Wpointer-arith -Werror -pipe
CFLAGSopt +=  -fomit-frame-pointer
#DEBUGGING OPTIONS (-pg -a for sampling)
CFLAGSdbg  =  -g -Wall -Wpointer-arith -Werror -pipe $(PIC) $(CPU)
CFLAGSdbg +=  -DTDEBUG $(SAMPLING)
#Grind profiler
CFLAGSgri  =  -g -O3 -Wall -Wpointer-arith -Werror -pipe $(PIC) $(CPU)

DLIB    = $(PIGALEPATH)/lib/lib$L$(OPT).so
LIBS    = -L$(PIGALEPATH)/lib -ltgraph$(OPT)
CFLAGS  = $(CFLAGS$(OPT))
VPATH   = ./.obj
FILES   :=  $(wildcard *.cpp)
SPECIAL := PropName.cpp
FILES   := $(filter-out $(SPECIAL), $(FILES))
FILES2  := $(FILES)
FILES2  += $(SPECIAL)
OBJECTS =  $(patsubst %.cpp,%.$(OPT), $(FILES2))
OBJ =  $(patsubst %.$(OPT),$(VPATH)/%.$(OPT), $(OBJECTS))

all : md depend $(DLIB)

PropName.cpp : $(PIGALEPATH)/incl/TAXI/propdef.h
	$(AWK) -f propname.awk $(PIGALEPATH)/incl/TAXI/propdef.h > PropName.cpp

.cpp.$(OPT): $*.cpp 
	$(CC) -c $(CFLAGS) $(IFLAGS) -o  $(VPATH)/$*.$(OPT)   $*.cpp

$(DLIB):	 $(OBJECTS)
	@rm -f $(PIGALEPATH)/lib/lib$L$(OPT).so
	$(CC) -shared $(LINK) $(SAMPLING) -o $(PIGALEPATH)/lib/lib$L$(OPT).so  $(OBJ)

clean:
	@rm -f *~ *bak *.flc $(OBJ) *.flc $(DLIB)

depend:
	@makedepend -o.$(OPT) -f gnumakefile $(IFLAGS)  -Y $(FILES) 2> /dev/null

nodepend:
	@makedepend -o.$(OPT) -f gnumakefile  -Y  2> /dev/null

list:
	../makelist *.cpp >Make.lst

md:
	@if ! test -d .obj ; then mkdir .obj ; fi
	@if ! test -d ../lib ; then mkdir ../lib ; fi	

static:$(OBJECTS)
	@ar cru $(PIGALEPATH)/lib/lib$L$(OPT).a  $(OBJ)
	@ranlib $(PIGALEPATH)/lib/lib$L$(OPT).a

test:
	@echo OBJ=$(OBJ)
	@echo OBJECTS=$(OBJECTS)

.PHONY: md all
# DO NOT DELETE

